pre {{ rows | json }}



table( role="grid" aria-labelledby="{{::uniqueId}}-title" aria-activedescendant="{{activeDateId}}" )
    thead
        tr
            th.text-center( ng-if="showWeeks" )
            th.text-center( ng-repeat="label in ::labels track by $index" )
                small( aria-label="{{::label.full}}" ) {{::label.abbr}}
    tbody
        tr.uib-weeks(  ng-repeat="row in rows track by $index" role="row" )
            td.text-center.h6( ng-if="showWeeks" )
                em {{ weekNumbers[$index] }}
            td.uib-day.text-center( ng-repeat="dt in row" role="gridcell" id="{{::dt.uid}}" ng-class="::dt.customClass" )
                button.btn.btn-default( uib-is-clas="'btn-info' for selectedDt, 'active' for activeDt on dt" ng-click="select(dt.date)" ng-disabled="::dt.disabled" tabindex="-1" )
                    span( ng-class="::{'text-muted': dt.secondary, 'text-info': dt.current}" ) {{::dt.label}}
                    required).form-control
        .form-group.col-sm-6.col-lg-4
            label.col-md-4
                p(ng-class="lifeCtrl.txtClass" style="font-size: 13px; font-weight: bold; padding-top:8px;") Municipio
            .col-md-8
                select(name="listPoblaciones"
                    ng-model="lifeCtrl.municipioAsegurado"
                    ng-options="x.text for x in lifeCtrl.ListPoblacionesAsegurado"
                    ng-disabled="lifeCtrl.isContratanteAsegurado"
                    ng-change="lifeCtrl.changePostalCode(true)"
                    required).form-control
        .form-group.col-sm-6.col-lg-4
            label.col-md-4
                p(ng-class="lifeCtrl.txtClass" style="font-size: 13px; font-weight: bold; padding-top:8px;") Código Postal
            .col-md-8
                select(name="ListCodPostalAsegurado"
                    ng-model="lifeCtrl.codigoPostalAsegurado"
                    ng-disabled="lifeCtrl.isContratanteAsegurado"
                    ng-options="x.text for x in lifeCtrl.ListCodPostalAsegurado"
                    required).form-control
        .clearfix.visible-lg
        .form-group.col-sm-6.col-lg-4
            label.col-md-4
                p(ng-class="lifeCtrl.txtClass" style="font-size: 13px; font-weight: bold; padding-top:8px;") País
            .col-md-8

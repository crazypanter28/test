      .row
        .col-md-12
          .form-group.col-lg-5
            .col-md-5
              p.text-right(style='font-size: 13px; font-weight: bold; padding-top:8px;') *Estado
            .col-md-7.text-left
              div(act-dropdown='act-dropdown', 
                ng-model='medicalCtrl.ListMedicalEstados', 
